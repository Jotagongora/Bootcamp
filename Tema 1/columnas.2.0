Proceso sin_titulo
	Definir columnas, filas, lados Como Entero;
	Escribir "Introduce el tamaño cuadrado.";
	Leer Lados;
	Para filas = 1 hasta lados Con Paso 1 Hacer
		Para columnas =0 Hasta lados Con Paso 1 Hacer
			si filas == columnas O (lados - filas + 1 == columnas) Entonces
				Escribir "x " Sin Saltar;
			SiNo
				Escribir "  " Sin Saltar;
			FinSi
		FinPara
		Escribir "  ";
	FinPara
FinProceso
